{# qui si definisce come formattare il form con nome indicato in form_widget e con route che rimanda al controller definita in path #}

<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<title>Cli list configuration</title>
		<link rel="stylesheet" href="/css/Style1.css" />
	</head>
  
	<body>
		<div class="wrap">
			<h2>Cli list configuration</h2>
			<form action="{{ path('faxcli') }}" method="post" {{ form_enctype(list_form) }}>
					{{ form_widget(list_form) }}  {{ form_errors(list_form) }} {{error_msg}}
				<br>
					<input type="submit" />
			</form>
			<form method="link" action="{{ path('pos') }} "><input type="submit" value=" Skip fax configuration"></form>
		</div>
		<aside class="summary">
			<h2>Summary</h2>
			{{summary.printUserXML}}
		</aside>

    </body>
