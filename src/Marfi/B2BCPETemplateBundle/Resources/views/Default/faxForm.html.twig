{# qui si definisce come formattare il form con nome indicato in form_widget e con route che rimanda al controller definita in path #}

<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<title>Fax service configuration</title>
		<link rel="stylesheet" href="/css/Style1.css" />
	</head>
  
	<body>
		<div class="central">
			<h2>Fax service configuration</h2>
			<form action="{{ path('fax') }}" method="post" {{ form_enctype(fax_form) }}>
					{{ form_widget(fax_form) }}  {{ form_errors(fax_form) }}
				<br>
					<input type="submit" />
			</form>
			<form method="link" action="{{ path('pos') }} "><input type="submit" value=" Skip fax configuration"></form>

		</div>

    </body>
